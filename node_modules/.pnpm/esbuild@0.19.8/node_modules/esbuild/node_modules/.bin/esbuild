else
  export NODE_PATH="/home/mastoora/Mastoora/Scrimba/React Challenges-4-project/coa3e46aaa9e0b9a1bf742870/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/bin/node_modules:/home/mastoora/Mastoora/Scrimba/React Challenges-4-project/coa3e46aaa9e0b9a1bf742870/node_modules/.pnpm/esbuild@0.19.8/node_modules/esbuild/node_modules:/home/mastoora/Mastoora/Scrimba/React Challenges-4-project/coa3e46aaa9e0b9a1bf742870/node_modules/.pnpm/esbuild@0.19.8/node_modules:/home/mastoora/Mastoora/Scrimba/React Challenges-4-project/coa3e46aaa9e0b9a1bf742870/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../bin/esbuild"   "$@"
